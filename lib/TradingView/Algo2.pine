    iBuy__SL_Points   := Fixed_SL_Points  
    iSell__SL_Points  := Fixed_SL_Points  
else
    iBuy__SL_Points   := math.max(iBuy__SL_Points  , Minimum_SL_Points)
    iSell__SL_Points  := math.max(iSell__SL_Points , Minimum_SL_Points)


